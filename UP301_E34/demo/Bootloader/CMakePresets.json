{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 19
  },
  "configurePresets": [
    {
      "name": "riscv-debug",
      "displayName": "RISCV Debug",
      "description": "Use RISCV toolchain for debug build",
      "generator": "Unix Makefiles",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/riscv-toolchain.cmake",
        "USE_LOAD_E21_CODE" : "ON",
        "E21_PROJECT": "E21_Testchip",
        "USE_METAL_STATIC_LIBS": "EXTERNAL",
        "USE_AUTO_WDT_KICK": "OFF",
        "BOARD_NAME": "Trina-pi"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "riscv-debug-build",
      "configurePreset": "riscv-debug"
    }
  ]
}